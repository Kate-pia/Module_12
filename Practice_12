money = float(input("Введите вносимую сумму денег:")) #Сумма денег, которую клиент хочет положить под проценты в банк
per_cent = {'ТКБ': 5.6, 'СКБ': 5.9, 'ВТБ': 4.28, 'СБЕР': 4.0} #Процентные ставки по вкладам в различных банках
value = list(per_cent.values()) #Извлечение процентных ставок из словаря в список
deposit = list()
for i in range(len(value)):
    deposit.append(int((money/100) * float(value[i]))) #
print(f'Доход по депозиту в разных банках {deposit}') #накопленные средства за год вклада в каждом из банков
print(f"Максимальная сумма за депозит: {max(deposit)}")
